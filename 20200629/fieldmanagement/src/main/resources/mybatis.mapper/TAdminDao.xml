<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.jxust.dao.TAdminDao">
    
    
    <select id="checkAdmin" parameterType="cn.edu.jxust.entity.TAdmin" resultType="cn.edu.jxust.entity.TAdmin">
        SELECT  * FROM  t_admin  where  admin_name = #{admin_name} AND
               admin_pwd  = #{admin_pwd}

    </select>

</mapper>